    <script>

  $(document).ready(function () {
      $('.nav > li.user-is-admin > p.menu-button-split-user').addClass('red-background');

                $('ul.dropdown-menu [data-toggle=dropdown]').on('click', function (event) {
                    event.preventDefault();
                    event.stopPropagation();
                    $(this).parent().siblings().removeClass('open');
                    $(this).parent().toggleClass('open');
                });

                //alert($("#lastdeployedtime").attr('data-value'));
                var deployedTime = moment($("#lastdeployedtime").attr('data-value'));
                $("#lastdeployedtime").attr("title",deployedTime.format('DD/MM/YYYY h:mm:ss A'));
                $("#lastdeployedtime").html(deployedTime.fromNow());
            });

  var token = '';
  console.log(token);
  if (token) {
    localStorage.setItem("access_token", token);
  }
    </script>
