<script>

    $(document).ready(function () {

        // Angular staff
        var map = {
            '@angular': '/_dist/node_modules/@angular',
            'rxjs': '/_dist/node_modules/rxjs',
            "angular2-jwt": "/_dist/node_modules/angular2-jwt/angular2-jwt.js",
            "Moment": "/_dist/Scripts/NuGet/Moment.js.2.8.2/moment.js",
        };
        // packages tells the System loader how to load when no filename and/or no extension
        var packages = {
            '@angular/router': { main: 'index.js', defaultExtension: 'js' },
            'rxjs': { defaultExtension: 'js' },
        };

        // List packages we use with angular here...
        var ngPackageNames = [
          'common',
          'compiler',
          'core',
          'http',
          'platform-browser',
          'platform-browser-dynamic',
          'upgrade',
          'forms'
        ];

        // Then add them to bundles for system.js
        ngPackageNames.forEach(function (pkgName) {
            packages['@angular/' + pkgName] = { main: 'bundles/' + pkgName + '.umd.js', defaultExtension: 'js' };
        });

        // No umd for router yet
        packages['@angular/router'] = { main: 'index.js', defaultExtension: 'js' };

        // Configure System.js
        System.config({
            defaultJSExtensions: "true",
            transpiler: "typescript",
            map: map,
            packages: packages
        });

        // Cache busting to ignore old JS files on the client's browser cache
        // Below is overriding System.locate function and should be avoided in future (hense the var name)
        // The System.locate can be found at line 155 of https://github.com/systemjs/systemjs/blob/master/lib/core.js
        var myTempFixForCacheBusting = System.locate;
        System.locate = function (load) {
            return Promise.resolve(myTempFixForCacheBusting.call(this, load)).then(function (address) {
                return address + '?ver=' + '1.50.32680';
            });
        };

        // Import main.js (which reads the window.view.config set by the chtml view)
        System.import('/_dist/app/js/main').then(null, console.error.bind(console));

    });
</script>





