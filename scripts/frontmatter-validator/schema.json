          "type": "string"
        }
      },
      "related": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "redirects": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "required": ["type", "title", "guid", "uri", "created", "authors"],
    "additionalProperties": true
}
  